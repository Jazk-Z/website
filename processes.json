{
  "apps": [
    {
      "name": "website",
      "script": "./server.js",
      "log_date_format": "YYYY-MM-DD HH:mm:SS",
      "log_file": "logs/website.log",
      "error_file": "logs/website-err.log",
      "out_file": "logs/website-out.log",
      "pid_file": "pids/website.pid",
      "instances": 1,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "1M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "cluster_mode",
      "autorestart": true,
      "vizion": false
    }
  ]
}
